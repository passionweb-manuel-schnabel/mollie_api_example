{
    "name": "passionweb/mollie-api",
    "license": "GPL-3.0",
    "description": "Shows the integration of the Mollie API for payments. (TYPO3 CMS)",
    "type": "typo3-cms-extension",
    "homepage": "https://www.passionweb.de",
    "version": "1.0.0",
    "authors": [
        {
            "name": "Manuel Schnabel",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": "^11.5 | ^12.4",
        "mollie/mollie-api-php": "^2.55"
    },
    "autoload": {
        "psr-4": {
            "Passionweb\\MollieApi\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "mollie_api"
        }
    }
}
